{
	"headers": [
		{
			"source": "/(.*).(css|js)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				},
				{
					"key": "Accept-Encoding",
					"value": "gzip, br"
				}
			]
		},
		{
			"source": "/(.*).(jpg|jpeg|png|webp|avif|gif|svg|woff2|woff|ttf|eot|otf)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600, must-revalidate"
				}
			]
		}
	]
}
